@model Vote

@{

    ViewData["Title"] = "Voting Page";
}
<p>@ViewData["Title"]</p>


<hr />
<form asp-action="Create">
    <div class="container">
        <div class="mb-2">
            <lable asp-for="StudentID" class="control-lable">Student ID</lable>
            <input asp-for="StudentID" class="form-control" />
            <span asp-validation-for="StudentID" class="text-danger"></span>
        </div>

         
        
         <div class="form-group">
        <label for="NomineeName">Select Nominee</label>
    @Html.DropDownListFor(model => model.NomineeName, (IEnumerable<SelectListItem>)ViewBag.Nominees, "Select Nominee", new { @class = "form-control" })
    </div>



        
            <input type="submit" value="Create" class="btn btn-primary btn-sm" />
        </div>
 
</form>



